{
  "name": "next-starter-template",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-05-05",
  "compatibility_flags": ["nodejs_compat"],
  "minify": true,
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "env": {
    "test": {
      "workers_dev": true
    },
    "production": {
      "route": {
        "pattern": "chattts.net",
        "zone_name": "chattts.net",
        "custom_domain": true
      }
    }
  }
}
